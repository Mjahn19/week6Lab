    "name = input(\"What is your name?\")\n",
    "number = input(\"Pick a number (1-50)\")\n",
    "number = int(number)\n",
    "while number < 1 or number > 50:\n",
    "    number = input(\"Invalid input try again\")\n",
    "    number = int(number)\n",
    "numSqr = number * number\n",
    "print(name, \", \", number, \" squared is \", numSqr)"
